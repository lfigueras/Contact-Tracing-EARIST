<%= form_for(@visitors) do |form| %>
<div class="mt-10 sm:mt-0 p-10">
  <div class="md:grid md:grid-cols-3 md:gap-6">
    <div class="md:col-span-1">
      <div class="px-4 sm:px-0">
        <h3 class="text-lg font-medium leading-6 text-gray-900">Personal Information</h3>
        <p class="mt-1 text-sm text-gray-600">
          Use a valid email address where you can receive email.
        </p>
      </div>
    </div>
    <div class="mt-5 md:mt-0 md:col-span-2">
      <form action="#" method="POST">
        <div class="shadow overflow-hidden sm:rounded-md">
          <div class="px-4 py-5 bg-white sm:p-6">
            <div class="grid grid-cols-6 gap-6">
              <div class="col-span-6 sm:col-span-3">
                <label for="first-name" class="block text-sm font-medium text-gray-700">Name</label>
                <%=form.text_field :name, :class=>"w-full px-5 py-1 text-gray-700 bg-gray-200 rounded", :id=>"cus_name",  :type=>"text", :required=>"", :placeholder=>"Your Name", :"aria-label"=>"Name", :style=>"background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;" %>
                </div>
                <div class="col-span-6 sm:col-span-3">
                <label for="email-address" class="block text-sm font-medium text-gray-700">Age</label>
                <%=form.text_field :age, :class=>"w-full px-5 py-1 text-gray-700 bg-gray-200 rounded", :id=>"cus_age",  :type=>"text", :required=>"", :placeholder=>"Type here", :"aria-label"=>"Age", :style=>"background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;" %>
                </div>
              <div class="col-span-6 sm:col-span-3">
                <label for="last-name" class="block text-sm font-medium text-gray-700">Email</label>
                <%=form.text_field :email, :class=>"w-full px-5 py-1 text-gray-700 bg-gray-200 rounded", :id=>"cus_email",  :type=>"text", :required=>"", :placeholder=>"Your Email", :"aria-label"=>"Email", :style=>"background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;" %>
                </div>
                <div class="col-span-6 sm:col-span-3">
                <label for="email-address" class="block text-sm font-medium text-gray-700">Contact Number</label>
                <%=form.text_field :phone_num, :class=>"w-full px-5 py-1 text-gray-700 bg-gray-200 rounded", :id=>"cus_number",  :type=>"text", :required=>"", :placeholder=>"Your Contact Number", :"aria-label"=>"Phone number", :style=>"background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;" %>
                </div>
                <div class="col-span-6 sm:col-span-3">
                <label for="email-address" class="block text-sm font-medium text-gray-700">Body Temperature</label>
                <%=form.text_field :temperature, :class=>"w-full px-5 py-1 text-gray-700 bg-gray-200 rounded", :id=>"cus_temp",  :type=>"text", :required=>"", :placeholder=>"Your Body Temperature", :"aria-label"=>"Body Temperature", :style=>"background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;" %>
                </div>
                <div class="col-span-6 sm:col-span-3">
                <label for="email-address" class="block text-sm font-medium text-gray-700">Occupation</label>
                <%=form.text_field :occupation, :class=>"w-full px-5 py-1 text-gray-700 bg-gray-200 rounded", :id=>"cus_occupation",  :type=>"text", :required=>"", :placeholder=>"Type here", :"aria-label"=>"Occupation", :style=>"background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;" %>
                </div>
                <div class="col-span-6 sm:col-span-4">
                <label for="email-address" class="block text-sm font-medium text-gray-700">Valid ID Number</label>
                <%=form.text_field :id_num, :class=>"w-full px-5 py-1 text-gray-700 bg-gray-200 rounded", :id=>"cus_id",  :type=>"text", :required=>"", :placeholder=>"Type here", :"aria-label"=>"ID number", :style=>"background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;" %>
                </div>

              <div class="col-span-6 sm:col-span-4">
                <label for="email-address" class="block text-sm font-medium text-gray-700">Address</label>
                <%=form.text_field :address, :class=>"w-full px-5 py-1 text-gray-700 bg-gray-200 rounded", :id=>"cus_address",  :type=>"text", :required=>"", :placeholder=>"Type here", :"aria-label"=>"Address", :style=>"background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;" %>
                </div>
            </div>
          </div>
          <div class="px-4 py-3 bg-gray-50 text-right sm:px-6">
          <%=form.submit :Submit, :class => "inline-flex justify-center py-2 px-4 border border-transparent shadow-sm text-sm font-medium rounded-md text-white bg-red-900 hover:bg-red-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-indigo-500 " %> 
          </div>
        </div>
      </form>
    </div>
  </div>
</div>
<%end%>
<a class="inline-block flex mt-5 justify-center py-3 px-5 text-gray-700 mb-4 font-bold text-xl border-gray-300 bg-gray-200  hover:bg-gray-100 " href="/healths/new" ,="" data-remote="true">Go to Health Declaration Form</a>
